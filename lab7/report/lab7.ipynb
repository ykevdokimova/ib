    "def gamm(t_text, t_key):\n",
    "    res = ''\n",
    "    for i, j in zip(t_text, t_key):\n",
    "        temp=chr(ord(i) ^ ord(j))\n",
    "        res+=temp\n",
    "    return res\n",
    "\n",
    "P1 = 'С Новым годом, друзья!'\n",
    "P2 = ''\n",
    "Key = 'лабораторная работа н7'\n",
    "\n",
    "P2 = gamm(P1, Key)\n",
    "print(P2)\n",
    "\n",
    "p_result = gamm(P2, P1)\n",
    "print(p_result)"
   ]
